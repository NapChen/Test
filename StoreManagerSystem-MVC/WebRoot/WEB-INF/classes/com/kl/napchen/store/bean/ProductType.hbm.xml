<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kl.napchen.store.bean.ProductType" table="t_product_type">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="type" type="java.lang.String">
			<column name="type" length="2" />
		</property>
		<property name="largeName" type="java.lang.String">
			<column name="large_name" length="256" />
		</property>
		<property name="largeType" type="java.lang.String">
			<column name="large_type" length="6" />
		</property>
		<property name="mediumName" type="java.lang.String">
			<column name="medium_name" length="256" />
		</property>
		<property name="mediumType" type="java.lang.String">
			<column name="medium_type" length="12" />
		</property>
		<property name="smallType" type="java.lang.String">
			<column name="small_type" length="18" />
		</property>
		<property name="smallName" type="java.lang.String">
			<column name="small_name" length="256" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="create_time"></column>
		</property>


	</class>
</hibernate-mapping>
